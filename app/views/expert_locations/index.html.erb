<h1>Listing expert_locations</h1>

<table>
  <tr>
    <th>Country</th>
    <th>Addressline1</th>
    <th>Addressline2</th>
    <th>Postalcode</th>
    <th>City</th>
    <th>Phone</th>
  </tr>

<% @expert_locations.each do |expert_location| %>
  <tr>
    <td><%=h expert_location.country %></td>
    <td><%=h expert_location.addressline1 %></td>
    <td><%=h expert_location.addressline2 %></td>
    <td><%=h expert_location.postalcode %></td>
    <td><%=h expert_location.city %></td>
    <td><%=h expert_location.phone %></td>
    <td><%= link_to 'Show', expert_location %></td>
    <td><%= link_to 'Edit', edit_expert_location_path(expert_location) %></td>
    <td><%= link_to 'Destroy', expert_location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New expert_location', new_expert_location_path %>