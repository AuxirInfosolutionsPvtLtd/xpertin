<h1>Listing provider_locations</h1>



  
<table>
  <tr>
  </tr>
<% @provider_locations.each do |provider_location| %>
<tr><td><%=h provider_location.country %></td></tr>
  <tr>
    <td><%= link_to 'Show', provider_location %></td>
    <td><%= link_to 'Edit', edit_provider_location_path(provider_location) %></td>
    <td><%= link_to 'Destroy', provider_location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New provider_location', new_provider_location_path %>